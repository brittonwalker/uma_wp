{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "uma-blocks/testimonial",
	"version": "0.1.0",
	"title": "Testimonial",
	"category": "design",
	"description": "A single testimonial.",
	"textdomain": "testimonial",
	"parent": [
		"uma-blocks/testimonials"
	],
	"attributes": {
		"author": {
			"type": "object",
			"properties": {
				"name": {
					"type": "string"
				},
				"role": {
					"type": "string"
				}
			},
			"default": {
				"name": "",
				"role": ""
			}
		},
		"photoUrl": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
