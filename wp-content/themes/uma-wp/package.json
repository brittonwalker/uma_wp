{
  "name": "my-theme",
  "version": "1.0.0",
  "description": "Starter development theme for WordPress",
  "author": "Britton Walker",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/brittonwalker/my-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brittonwalker/my-theme.git"
  },
  "bugs": {
    "url": "https://github.com/brittonwalker/my-theme/issues"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "main": "index.js",
  "dependencies": {
    "@barba/core": "^2.10.3",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/postcss": "^4.1.13",
    "gsap": "^3.12.7",
    "headroom.js": "^0.12.0",
    "lenis": "^1.2.1",
    "postcss": "^8.5.6",
    "prismjs": "^1.29.0",
    "split-type": "^0.3.4",
    "swiper": "^11.2.10",
    "three": "^0.174.0",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@wordpress/eslint-plugin": "^22.5.0",
    "@wordpress/prettier-config": "^4.19.0",
    "@wordpress/scripts": "^30.12.0",
    "@wordpress/stylelint-config": "^23.11.0",
    "browser-sync": "^3.0.3",
    "css-loader": "^7.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "html-webpack-plugin": "^5.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-nested": "^7.0.2",
    "prettier-plugin-twig-melody": "^0.4.6",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.15.0",
    "tailwindcss": "^4.1.13",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "wp-scripts build src/index.js",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "dev": "npm run watch",
    "lint": "run-p lint:*",
    "format": "run-p format:*",
    "format:css": "npm run lint:css -- --fix",
    "format:php": "composer run-script format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "composer run-script lint",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "start": "wp-scripts start src/index.js",
    "start:hmr": "webpack serve --config webpack.dev.js",
    "sync": "browser-sync start --https --proxy https://uma-wp.test --no-open --files '**/*.html, **/*.php, **/*.twig, !node_modules/**/*.html'",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "watch": "run-p start sync",
    "debug-log": "tail -f ../../debug.log"
  }
}
